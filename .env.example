ADMIN_EMAILS=admin@example.com,admin2@example.com

# Firebase Service Account Key (JSON format)
FIREBASE_SERVICE_ACCOUNT_KEY={"type":"service_account","project_id":"your-project-id","private_key_id":"your-key-id","private_key":"your-private-key","client_email":"your-client-email","client_id":"your-client-id"} 